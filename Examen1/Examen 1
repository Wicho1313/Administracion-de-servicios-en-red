// Red Examen 1 parcial
digraph {
	edge [arrowhead=none]
	R1 [fillcolor=antiquewhite3 shape=cylinder style=filled]
	R1 -> R2
	R2 [fillcolor=antiquewhite3 shape=cylinder style=filled]
	R1 -> R2
	R1 [fillcolor=antiquewhite3 shape=cylinder style=filled]
	PCV [color=mediumblue fillcolor=lightblue shape=box style=filled]
	Sw [color=lightblue fillcolor=darkolivegreen3 orientation=180 shape=box3d style=filled]
	PCV -> Sw
	Sw -> R1
	Sw -> R4
	PCV [color=mediumblue fillcolor=lightblue shape=box style=filled]
	Sw [color=lightblue fillcolor=darkolivegreen3 orientation=180 shape=box3d style=filled]
	PCV -> Sw
	Sw -> R1
	Sw -> R4
	R2 [fillcolor=antiquewhite3 shape=cylinder style=filled]
	PCR2 [color=mediumblue fillcolor=lightblue shape=box style=filled]
	PCR2 -> R2
	R4 [fillcolor=antiquewhite3 shape=cylinder style=filled]
	R4 -> R6
	R6 [fillcolor=antiquewhite3 shape=cylinder style=filled]
	R4 -> R6
	R4 [fillcolor=antiquewhite3 shape=cylinder style=filled]
	PCR6 [color=mediumblue fillcolor=lightblue shape=box style=filled]
	PCR6 -> R6
}
